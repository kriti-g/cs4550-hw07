<p>
  <span><%= link "Edit", to: Routes.event_path(@conn, :edit, @event) %></span>
  <span><%= link "Back", to: Routes.event_path(@conn, :index) %></span>
</p>

<h1>Show Event #<%= @event.id%></h1>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @event.name %>
  </li>

  <li>
    <strong>User:</strong>
    <%= @event.user %>
  </li>

  <li>
    <strong>Date:</strong>
    <%= @event.date %>
  </li>

</ul>

<div>
    <strong>Desc:</strong>
    <%= @event.desc %>
</div>
